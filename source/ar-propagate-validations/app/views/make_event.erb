<form action="/events/create" method="post">
  Organizer Name: <input type="text" name="organizer_name">
  Organizer Email: <input type="email" name="organizer_email">
  Title of Event: <input type="text" name = "title">
  Date of Event: <input type="date" name="date">
  <input type="submit" value="submit">
</form>

<% if @errors %>
  <ul>
    <% @errors.each do |field, error| %>
    <li> <%= error %> </li>
    <% end %>
  </ul>
<% end %>